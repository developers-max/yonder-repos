# Backend database URL
# DATABASE_URL=postgres://user:pass@host:5432/dbname

# Which physical tables to use for plots data
# Allowed: prod | stage
# Default is stage if unset
PLOTS_TABLE_ENV=stage

# Public app URL (used in emails and links)
# NEXT_PUBLIC_APP_URL=http://localhost:3000

# Max organizations (projects) a user can create
# ORGANIZATION_LIMIT=10

# Gemini API key for image selection
# GEMINI_API_KEY=

# Optional: Google Custom Search for image fallback
# GOOGLE_API_KEY=
# GOOGLE_CSE_CX=
